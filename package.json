{
  "name": "votetracker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:assets": "babel-node tools/build-assets",
    "clean": "rimraf public",
    "start": "webpack-dev-server --hot --inline --progress --colors --content-base src",
    "lint": "eslint --ext .js,.jsx .",
    "prebuild": "npm run clean && npm run build:assets",
    "prestart": "npm run clean",
    "shrinkwrap": "npm shrinkwrap && babel-node tools/fix-shrinkwrap"
  },
  "dependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "bluebird": "^3.2.1",
    "css-loader": "^0.23.1",
    "fs-extra": "^0.26.5",
    "history": "^2.0.0-rc2",
    "nib": "^1.1.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^2.0.0-rc5",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "webpack": "^1.12.12"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-register": "^6.4.3",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^4.1.0",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.14.1"
  }
}
